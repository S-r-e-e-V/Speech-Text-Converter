<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.speech.voice_text">

    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/speech_text_icon_white_round"
        android:label="Speech-Text Converter"
        android:roundIcon="@mipmap/speech_text_icon_white_round"
        android:supportsRtl="true"
        android:versionCode="2"
        android:versionName="2.0"
        android:theme="@style/AppTheme">
        <activity android:name="com.speech.voice_text.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>